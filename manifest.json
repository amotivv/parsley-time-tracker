{
  "manifest_version": 3,
  "name": "Work Time Tracker",
  "description": "This extension will help you track your work time.",
  "version": "1.0",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
